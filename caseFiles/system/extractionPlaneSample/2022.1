extractionPlaneSample
{
    type                surfaces;
    libs                ("libsampling.so");
    executeControl      runTime;
    executeInterval     0.0025;
    writeControl        runTime;
    writeInterval       0.0025;
    timeStart           1; // First sample serves as TP /0
    // timeEnd             0;
    enabled             true;

    interpolationScheme     cellPoint;
    surfaceFormat           foam;

    fields
    (
        U
        nut
        nuTilda
    );

    surfaces
    (
        thirdPartyExtraction
        {
            type        cuttingPlane;
            planeType   pointAndNormal;

            pointAndNormalDict
            {
                basePoint       (2.29025 0 0);
                normalVector    (1 0 0);
            }

            interpolate false;
        }
    );
}

