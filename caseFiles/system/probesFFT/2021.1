probesWake
{
	type			probes;
	libs			("libsampling.so");
	executeControl	timeStep;
	executeInterval	1;
	writeControl	timeStep;
	writeInterval	1;
	timeStart		1;
	timeEnd			5.00002;	// Offset by +ve dT
	enabled			true;

	fields
	(
		U
	);

	probeLocations
	(
		(0.628 0 0.059)			// Centre-bottom
		(0.628 0 0.1945)		// Centre-mid
		(0.628 0 0.33)			// Centre-top
		(0.628 -0.135 0.1945)	// Left-mid
		(0.628 0.135 0.1945)	// Right-mid
	);
}

