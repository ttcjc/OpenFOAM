probesBL
{
	type			sets;
	libs			("libsampling.so");
	executeControl	runTime;
	executeInterval	0.0025;
	writeControl	runTime;
	writeInterval	0.0025;
	timeStart		1;
	timeEnd			5.00002;	// Offset by +ve dT
	enabled			true;

	interpolationScheme		cellPoint;
	setFormat				raw;

	fields
	(
		U
	);

	sets
	(
		boundaryLayerLocationA
		{
			type	lineUniform;
			axis 	z;
			nPoints	100;

			start   (-0.56075 0 0);
			end     (-0.56075 0 0.1);
		}

		boundaryLayerLocationB
		{
			type	lineUniform;
			axis 	z;
			nPoints	100;

			start   (0 0 0);
			end     (0 0 0.1);
		}
	);
}

