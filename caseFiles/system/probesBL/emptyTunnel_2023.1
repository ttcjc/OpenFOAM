probesBL
{
    type                sets;
    libs                ("libsampling.so");
    executeControl      runTime;
    executeInterval     0.0025;
    writeControl        runTime;
    writeInterval       0.0025;
    timeStart           0.5;
    // timeEnd             0;
    enabled             true;

    interpolationScheme     cellPoint;
    setFormat               raw;

    fields
    (
        U
    );

    sets
    (
        boundaryLayerLocationA
        {
            type        lineUniform;
            axis        z;
            nPoints     150;

            start       (-0.56075 0 0);
            end         (-0.56075 0 0.15);
        }

        boundaryLayerLocationB
        {
            type        lineUniform;
            axis        z;
            nPoints     150;

            start       (0 0 0);
            end         (0 0 0.15);
        }
    );
}

