FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Revision 2023.1
// Changelog: N/A
// Notes: N/A


solvers
{
    "(Phi.*|U.*|nuTilda.*)"
    {
        solver                  GAMG;
        smoother                GaussSeidel;
        tolerance               1e-8;
        relTol                  0;
        minIter                 1;
        maxIter                 50;

        cacheAgglomeration      yes;
        agglomerator            faceAreaPair;
        nCellsInCoursestLevel   20;
        directSolveCoarset      false;
        mergeLevels             1;

        nPreSweeps              0;
        nPostSweeps             1;
        nFinestSweeps           3;
    }

    p
    {
        solver                  GAMG;
        smoother                GaussSeidel;
        tolerance               1e-5;
        relTol                  0;
        minIter                 1;
        maxIter                 50;

        cacheAgglomeration      yes;
        agglomerator            faceAreaPair;
        nCellsInCoursestLevel   20;
        directSolveCoarset      false;
        mergeLevels             1;

        nPreSweeps              0;
        nPostSweeps             1;
        nFinestSweeps           3;
    }

    pFinal
    {
        solver                  GAMG;
        smoother                GaussSeidel;
        tolerance               2.5e-7;
        relTol                  0;
        minIter                 1;
        maxIter                 50;

        cacheAgglomeration      yes;
        agglomerator            faceAreaPair;
        nCellsInCoursestLevel   20;
        directSolveCoarset      false;
        mergeLevels             1;

        nPreSweeps              0;
        nPostSweeps             1;
        nFinestSweeps           3;
    }
}

relaxationFactors
{
}

cache
{
    grad(U);
    grad(nuTilda);
}

potentialFlow
{
    nNonOrthogonalCorrectors    4;
}

PISO
{
    momentumPredictor           yes;
    nCorrectors                 3;
    nNonOrthogonalCorrectors    1;
}

