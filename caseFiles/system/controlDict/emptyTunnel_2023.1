FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Revision emptyTunnel_2023.1
// Changelog: N/A
// Notes: N/A


application         pisoFoam;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             1.5;

deltaT              2e-5;

writeControl        runTime;

writeInterval       0.1;

purgeWrite          0;

writeFormat         ascii;

writePrecision      8;

writeCompression    off;

timeFormat          general;

timePrecision       8;

runTimeModifiable   true;

adjustTimeStep      false;

// ------------------------ //

libs
(
    "libfieldFunctionObjectsExtended.so"
);

functions
{
    // Fields
    #include        "k_subgrid"
    #include        "yPlus"
    #include        "fieldAverages"

    // Sampling
    #include        "probesBL"
}

