FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Revision 2022.1
// Changelog: N/A
// Notes: N/A


application         pisoFoam;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             0.25;

deltaT              2e-5;

writeControl        runTime;

writeInterval       0.25;

purgeWrite          0;

writeFormat         ascii;

writePrecision      8;

writeCompression    off;

timeFormat          general;

timePrecision       8;

runTimeModifiable   true;

adjustTimeStep      false;

// ------------------------ //

libs
(
    "libfieldFunctionObjectsCJC.so"
);

functions
{
    // Fields
    #include        "CourantNo"
    #include        "DESmodelRegions"
    #include        "k_subgrid"
    #include        "yPlus"
    #include        "fieldAverages"

    // Monitoring
    #include        "forceCoeffs"

    // Sampling
    // #include        "probesBL"
    #include        "extractionPlaneSample"
    #include        "probesPressure"
    #include        "probesVelocity"
}

