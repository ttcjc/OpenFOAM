FoamFile
{
	version		2.0;
	format		ascii;
	class		dictionary;
	location	"system";
	object		controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Revision 2021.1
// Changelog: N/A
// Notes: N/A

application			pisoFoam;

startFrom			latestTime;

startTime			0;

stopAt				endTime;

endTime				1;

deltaT				2e-5;

writeControl		runTime;

writeInterval		0.05;

purgeWrite			1;

writeFormat			ascii;

writePrecision		8;

writeCompression	off;

timeFormat			general;

timePrecision		8;

runTimeModifiable	true;

adjustTimeStep		false;

// ------------------------ //

libs
(
	"libfieldFunctionObjectsCJC.so"
);

functions
{
	#include		"CourantNo"
	#include		"yPlus"
	#include		"Qcriterion"
	#include		"fieldAverages"
	#include		"R"
	#include		"LESresolution"
	#include		"DESmodelRegions"
	#include		"LESaverages"
	#include		"forceCoeffs"
	#include		"planeSample"
	#include		"probesBL"
	#include		"probesFFT"
	#include		"probesPOD"
}

