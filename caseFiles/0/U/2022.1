FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Revision 2022.1
// Changelog: N/A
// Notes: N/A


dimensions      [0 1 -1 0 0 0 0]; // kg m s K mol A cd

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type        fixedValue;
        value       uniform (40.340020146898600 0 0);
    }

    outlet
    {
        type        zeroGradient;
    }

    "(inletSection.*|inletBuffer.*|outletBuffer.*|outletSection.*)"
    {
        type        slip;
    }

    "(workingSection.*|windsor.*)"
    {
        type        noSlip;
    }
}

